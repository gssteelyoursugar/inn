.swiper2{
  margin-left: 10rpx;
  margin-bottom: 40rpx;
}
.swiper2 swiper{
  height: 450rpx !important;
}
.swiper-items2{
  height: 450rpx !important;
}
.swiper2 .swiper-item2{
  height: 440rpx;
  margin: 0 15rpx 10rpx !important;
  border-radius: 10rpx;
  overflow: hidden;
  border: 2rpx solid #f5f5f5;
}
.item2-imgs{
  height: 350rpx;
  position: relative;
}
.item2-imgs .item2-imgs-url{
  width: 100%;
  height: 100%;
}
.item2-imgs .item2-imgs-sc{
  position: absolute;
  top: 30rpx;
  right: 30rpx;
  width: 55rpx;
  height: 55rpx;
}
.item2-desc{
  height: 90rpx;
  overflow: hidden;
}
.item2-desc .desc-warp{
  width: 95%;
  height: 90rpx;
  margin: 10rpx auto;
  display: flex;
  justify-content: space-between;
}
.desc-warp .warp-title{
  margin-top: 5rpx;
  width: 79%;
}
.desc-warp .warp-title .title-cont{
  font-size: 27rpx;
  color: #333333;
  font-weight: bold;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}
.desc-warp .warp-title .title-detals{
  display: flex;
}
.title-detals image{
  width: 30rpx;
  height: 30rpx;
}
.detals-location{
  width: 90%;
  font-size: 25rpx;
  color: #999999;
  font-weight: 400;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}

.item2-price{
  font-size: 32rpx;
  color: #ef5473;
}
.item2-price .item2-price-un{
  font-size: 24rpx;
}



