/* pages/index/index.wxss */
page{
  /* background: #f0f3f5; */
}
/*顶部搜索*/
.search{
  background: #ffffff;
  width: 750rpx;
  height: 80rpx;
  padding-top: 10rpx;
}
.search .search-warp{
  width: 700rpx;
  height: 70rpx;
  background: #f0f3f5;
  margin: 0 auto;
  border-radius: 40rpx;
  display: flex;
  align-items: center;
  color: #999999;
  font-size: 27rpx;
}
.search-warp image{
  margin-left: 30rpx;
  margin-right: 10rpx;
  width: 48rpx;
  height: 48rpx;
}

/*轮播图*/
.swiper{
  position: relative;
}
.swiper swiper{
  height: 700rpx !important;
}
.swiper-items{
  height:700rpx !important;
}
.swiper .swiper-item{
  height: 700rpx;
}
.swiper .swiper-item image{
  width: 100%;
  height: 100%;
}
.index-main{
  margin-top: 10rpx;
  background: #ffffff;
  width: 750rpx;
}
/*我要预定*/
.over{
  width: 700rpx;
  margin: 230rpx auto 30rpx;
  overflow: hidden;
  display: flex;
  justify-content: space-between;
}
.over .over-want{
  width: 330rpx;
  /* margin-top: 10rpx;
  margin-bottom: 10rpx;
  margin-left: 10rpx; */
  margin: 10rpx;
  height: 150rpx;
  align-items: center;
  box-sizing: border-box;
  display: flex;
  /* background: #fa24ef59; */
  border-radius: 15rpx;
  /* border: 1rpx solid #cccccc; */
  box-shadow: 0rpx 0rpx 20rpx 0rpx #f5f5f5;
}
.over-want .want-icon{
   margin-left: 20rpx;
    width: 80rpx;
    height: 80rpx;
    overflow: hidden;
    border-radius: 15rpx;
}
.want-icon image{
  width: 80rpx;
  height: 80rpx;
}
.want-name{
  margin-left: 15rpx;
}
.want-name .name-title{
  font-size: 30rpx;
  /* color: #ffffff; */
  color: #333333;
  font-weight: 500;
}
.want-name .name-desc{
  /* color: #ffffffe1; */
  color: #999999;
  font-size: 24rpx;
}
/*预定框*/
.reserve{
  width: 680rpx;
  height: 420rpx;
  padding-bottom: 30rpx;
  position: absolute;
  left: 0;
  right: 0;
  bottom: 0;
  top: 430rpx;
  margin: 0 auto;
  background: #ffffff;
  box-shadow: 0rpx 0rpx 8rpx 0rpx #cccccc;
  border-radius: 20rpx;
}
.reserve .reservebox{
  width: 600rpx;
  height: 420rpx;
  margin: 0 auto;
}
.reserve-city{
  display: flex;
  justify-content: space-between;
  border-bottom: 1rpx solid #cccccc96;
  align-items: center;
  height: 100rpx;
}
.reserve-city .city-title{
  color: #333333;
  font-size: 35rpx;
  font-weight: bold;
  width: 50%;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}
.reserve-city .city-select{
  width: 50%;
  display: flex;
  justify-content: flex-end;
}
.reserve-city .city-select .name{
  color: #999999;
  font-size: 25rpx;
  margin-right: 10rpx;
}
.reserve-city .city-select image{
  width: 30rpx;
  height: 30rpx;
}

.checkin-time{
  display: flex;
  justify-content: space-between;
  align-items: center;
  height: 120rpx;
  border-bottom: 1rpx solid #cccccc96;
}
.checkin-time .begin-title{
  text-align: center;
  margin-bottom: 5rpx;
  color: #999999;
  font-size: 25rpx;
}
.checkin-time .begin-time{
  font-size: 30rpx;
  text-align: center;
  color: #333333;
  font-weight: bold;
}
.chtime-num{
  color: #999999;
  font-size: 27rpx;
}
.chtime-num span{
  display: inline-block;
  min-width: 40rpx;
  text-align: center;
}

.res-give{
  height: 90rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  border-bottom: 1rpx solid #cccccc96;
  color: #999999;
  font-size: 26rpx;
}
.res-give image{
  width: 30rpx;
  height: 30rpx;
}

.res-button{
  margin: 30rpx auto 0;
  width: 400rpx;
  height: 80rpx;
  line-height: 80rpx;
  border-radius: 20rpx;
  background-image: linear-gradient(45deg,  #ffc95c,#ffd37b);
  color: #ffffff;
  text-align: center;
  font-size: 30rpx;
  font-weight: bold;
}

/*我已有订单*/
.existing{
  width: 700rpx;
  height: 108rpx;
  line-height: 108rpx;
  color: #333333;
  text-align: center;
  font-weight: 500;
  font-size: 30rpx;
  margin: 250rpx auto 30rpx;
  background:#fff3f3;
  border-radius: 25rpx;
}
.existingbox{
  width: 600rpx;
  margin: 0 auto;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.existing .existing-order{
  margin-left: 30rpx;
  width: 70rpx;
  height: 70rpx;
}
.existing .existing-arrows{
  width: 60rpx;
  height: 60rpx;
  margin-right: 30rpx;
}



/*本地房源*/
.housing{
  background: #ffffff;
}
.housing-bar{
  width: 700rpx;
  margin: 0 auto;
  display: flex;
  justify-content: space-between;
  height: 70rpx;
  line-height: 70rpx;
}
.bar-title{
  font-size: 32rpx;
  font-weight: bold;
  color: #333333;
}
.bar-more{
  display: flex;
}
.bar-more .more-title{
  color: #999999;
  font-size: 26rpx;
  font-weight: bold;
}
.bar-more image{
  margin: auto 0;
  width: 25rpx;
  height: 25rpx;
}

